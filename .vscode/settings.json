{
  "dart.flutterSdkPath": ".fvm/versions/3.32.4",
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "cSpell.words": [
    "easyhr",
    "dateTimeDifferance",
    "iattendance",
    "placemarks",
    "timezone",
    "riverpod",
    "camerawesome",
    "retrofit",
    "upgrader",
    "reorderable",
    "geolocator"
  ],
  "yaml.schemaStore.enable": false
}